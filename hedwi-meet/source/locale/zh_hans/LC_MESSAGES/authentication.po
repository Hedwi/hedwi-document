#: ../../source/authentication.rst:7
msgid "Authentication"
msgstr "鉴权"

#: ../../source/authentication.rst:10
msgid "1. Authentication type"
msgstr "1. 鉴权类型"

#: ../../source/authentication.rst:12
msgid "none: No authentication"
msgstr "none: 无鉴权"

#: ../../source/authentication.rst:13
msgid "ldap: LDAP authentication"
msgstr "ldap: LDAP 鉴权"

#: ../../source/authentication.rst:16
msgid "2. How to configure authentication"
msgstr "2. 如何配置鉴权"

#: ../../source/authentication.rst:17
msgid "set MEET_AUTH_TYPE in .env file"
msgstr "在 .env 文件中设置 MEET_AUTH_TYPE"

#: ../../source/authentication.rst:20
msgid "3. How to configure LDAP authentication"
msgstr "3. 如何配置 LDAP 鉴权"

#: ../../source/authentication.rst:22
msgid "set the following variables in .env file"
msgstr "在 .env 文件中设置以下变量"

#: ../../source/authentication.rst:24
msgid ""
"MEET_LDAP_ENABLED=\"true\"                  # whether to enable ldap "
"authentication MEET_LDAP_ADDR=\"ldap://127.0.0.1:42389\"   #ldap server "
"address MEET_LDAP_BASE_DN=\"dc=hedwi,dc=com\"       # ldap base dn"
msgstr "MEET_LDAP_ENABLED=\"true\"                  # 是否启用 LDAP 鉴权 MEET_LDAP_ADDR=\"ldap://127.0.0.1:42389\"   # LDAP 服务器地址 MEET_LDAP_BASE_DN=\"dc=hedwi,dc=com\"       # LDAP 基础 DN"

#: ../../source/authentication.rst:29
msgid "4. How to use authentication in mobile app"
msgstr "4. 如何在移动应用中使用鉴权"

#: ../../source/authentication.rst:31
msgid "ldap enabled, the mobile app will use ldap authentication"
msgstr "LDAP 鉴权启用，移动应用将使用 LDAP 鉴权"

#: ../../source/authentication.rst:33
msgid "password =  ldap password + \" \" + meeting password"
msgstr "密码 =  LDAP 密码 + \" \" + 会议密码"

#: ../../source/authentication.rst:36
msgid ""
"ldap disabled, the mobile app will use meeting password and if meeting "
"password is not set, leave it blank"
msgstr "LDAP 鉴权禁用，移动应用将使用会议密码，如果会议密码未设置，则留空"

#: ../../source/authentication.rst:38
msgid "password =  meeting password or blank"
msgstr "密码 =  会议密码或空白"

